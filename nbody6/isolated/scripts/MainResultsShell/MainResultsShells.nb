Cell["\<\
At a given critical time, multiples are assumed to be all binaries which in \
the previous time were inside.\
\>", "Text",
 CellChangeTimes->{{3.7474929422351637`*^9, 
  3.747493065691084*^9}},ExpressionUUID->"7008f6cb-8018-4dc2-b04f-\
d727b2494721"],

Cell["\<\

Do[
 
 nlag = n;
 times[nlag, \"C\"] = criticalTimes[nlag];
 multiples[nlag, \"C\"] = Symbol[\"criticalMultiples\" <> ToString[nlag]];
 
 outsideName = \"outside\" <> \"C\" <> ToString[nlag];
 Clear[Evaluate@outsideName];
 outside = Symbol[outsideName];
 
 dc2AtCriticalTimes[nlag, \"C\"] = dc2[[All, times[nlag, \"C\"]]];
 particlesAtCriticalTimes[nlag, \"C\"] = particles[[All, All, times[nlag, \"C\
\"]]];
 
 (* Add distance to density center *)
 multiples[nlag, \"C\"] = addDistanceToCenter[multiples[nlag, \"C\"], \
dc2AtCriticalTimes[nlag, \"C\"]];
 
 (* Generate downvalues *)
 Do[createOutsideValues[outside, nlag, #Name, #DistanceToDensityCenter, \
#Component1, #Component2, run, time] & /@ multiples[nlag, \"C\"][[run, \
time]], {run, 1, runmax}, {time, 1, nlag - 1}];
 
 gathered[nlag, \"C\"] = GatherBy[#, #[[2]] &] & /@ \
GatherBy[DeleteCases[ReleaseHold[DownValues[Evaluate[outside]][[All, 1]] /. \
outside -> List], {_Integer, _Integer, _Integer}], First];
 
 allBinaries[nlag, \"C\"] = \
SplitBy[SortBy[Flatten[binariesProduction[outside, #] & /@ gathered[nlag, \"C\
\"], 1], #[[2]] &], #[[2]] &];
 
 allBinariesTEST[nlag, \"C\"] = \
SplitBy[SortBy[Flatten[Most[binariesProduction[outside, #]] & /@ \
gathered[nlag, \"C\"], 1], #[[2]] &], #[[2]] &];
 
 stepBeforeName = \"stepbefore\" <> \"C\" <> ToString[nlag];
 Clear[Evaluate[stepBefore]];
 stepBefore = Symbol[stepBeforeName];
 
 stepBefore[_, 1, _] := True; stepBefore[run_, time_, name_] := Apply[And, \
MapAt[Norm[# - dc2[[run, criticalTimes[nlag][[time]] - 1]]] < \
limitRadii[nlag][[time - 1]] &, Cases[Transpose[particles[[{2, 4}, run, \
criticalTimes[nlag][[time]] - 1]]], {_, Alternatives @@ Flatten[name]}], \
{All, 1}][[All, 1]]];
 
 in[nlag, \"C\"] = Pick[allBinaries[nlag, \"C\"], Map[! (outside @@ #) &, \
allBinaries[nlag, \"C\"], {2}]]; out[nlag, \"C\"] = Pick[allBinaries[nlag, \
\"C\"], Map[outside @@ # &, allBinaries[nlag, \"C\"], {2}]]; newout[nlag, \"C\
\"] = Pick[out[nlag, \"C\"], MapAt[stepBefore @@ # &, out[nlag, \"C\"], {1 \
;;, All}]];
 
 inTEST[nlag, \"C\"] = Pick[allBinariesTEST[nlag, \"C\"], Map[! (outside @@ \
#) &, allBinariesTEST[nlag, \"C\"], {2}]]; outTEST[nlag, \"C\"] = \
Pick[allBinariesTEST[nlag, \"C\"], Map[outside @@ # &, allBinariesTEST[nlag, \
\"C\"], {2}]]; newoutTEST[nlag, \"C\"] = Pick[outTEST[nlag, \"C\"], \
MapAt[stepBefore @@ # &, outTEST[nlag, \"C\"], {1 ;;, All}]];
 
 allBinaries[nlag, \"C\"] = Join @@@ Transpose[{in[nlag, \"C\"], newout[nlag, \
\"C\"]}];
 
 allBinariesTEST[nlag, \"C\"] = Join @@@ Transpose[{inTEST[nlag, \"C\"], \
newoutTEST[nlag, \"C\"]}];
 
 scapedMultiples[nlag, \"C\"] = Flatten[#, 1] & /@ Map[Cases[multiples[nlag, \
\"C\"][[#1, #2]], <|\"Name\" -> #3, __|>] & @@ # &, allBinaries[nlag, \"C\"], \
{2}];
 
 scapedMultiplesTEST[nlag, \"C\"] = Flatten[#, 1] & /@ \
Map[Cases[multiples[nlag, \"C\"][[#1, #2]], <|\"Name\" -> #3, __|>] & @@ # &, \
allBinariesTEST[nlag, \"C\"], {2}];
 
 scapedMultiplesAll[nlag, \"C\"] = Flatten[scapedMultiples[nlag, \"C\"], 1];
 
 scapedMultiplesAllTEST[nlag, \"C\"] = Flatten[scapedMultiplesTEST[nlag, \
\"C\"], 1],
 
 {n, numberOfLagRadii}
 
 ]\
\>", "Program",
 CellChangeTimes->{{3.745266658421328*^9, 3.745266676318117*^9}, {
   3.74526671361399*^9, 3.745266714189893*^9}, {3.745266745373766*^9, 
