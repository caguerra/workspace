Cell["\<\
(* Finding critical times: old algorithm *)
Do[
 criticalTimes[n] =
  Which[
     Length[#] == 2, First[#],
     Length[#] == 3, #[[2]],
     Length[#] > 3, Ceiling[Mean[#]]
     ] & /@ ((Flatten[Cases[Partition[Transpose[{nbIndexArray, #}], 2, 1], \
{{x1_, y1_}, {x2_, y2_}} /; \[Rho]SunNeighbor <= y1 && y2 <= \
\[Rho]SunNeighbor], 1][[All, 1]] & /@ Reverse[lagdens[n]]) //. {x__, \
PatternSequence[{a_, b_}, {a_, b_}], y__} :> {x, {a, b}, {a, b} + 1, y});
 
 If[Length[DeleteDuplicates[criticalTimes[n]]] =!= n - 1, Print[\"critical \
times reached granularity for n = \", n]];
 
 limitRadii[n] = MapThread[Part, {Reverse@lradii[n], criticalTimes[n]}],
 
 {n, numberOfLagRadii}
 ]\
\>", "Program",
