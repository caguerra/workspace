  RowBox[{"Module", "[", "\[IndentingNewLine]", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"gatheredByRun", ",", "typesPerRun"}], "}"}], ",", 
    "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"gatheredByRun", "=", 
      RowBox[{"GatherBy", "[", 
       RowBox[{
        RowBox[{
         RowBox[{
          RowBox[{"{", 
           RowBox[{"#RunID", ",", "#Membership", ",", "#Name"}], "}"}], "&"}],
          "/@", "allBinairesWithMembership"}], ",", "First"}], "]"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"Total", "@", "\n", "\t", 
      RowBox[{"Table", "[", "\n", "\t", 
       RowBox[{
        RowBox[{"With", "[", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"cases", "=", 
            RowBox[{
             RowBox[{
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{
                   RowBox[{"#", "[", 
                    RowBox[{"[", "2", "]"}], "]"}], "/.", 
                   RowBox[{"{", 
                    RowBox[{
                    RowBox[{"\"\<B\>\"", "->", "2"}], ",", 
                    RowBox[{"\"\<T\>\"", "\[Rule]", "3"}], ",", 
                    RowBox[{"\"\<Q\>\"", "\[Rule]", "4"}]}], "}"}]}], ")"}], 
                 "\[NotEqual]", 
                 RowBox[{"Length", "[", 
                  RowBox[{"Flatten", "[", 
                   RowBox[{"#", "[", 
