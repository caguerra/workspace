              RowBox[{"[", "i", "]"}], "]"}]}]}], "}"}], ",", 
          RowBox[{"Flatten", "@", 
           RowBox[{"{", 
            RowBox[{
             RowBox[{"numberOfStars", "-", 
              RowBox[{"Length", "[", 
               RowBox[{"Flatten", "[", 
                RowBox[{"cases", "[", 
                 RowBox[{"[", 
                  RowBox[{"All", ",", "3"}], "]"}], "]"}], "]"}], "]"}]}], 
             ",", 
             RowBox[{
              RowBox[{
               RowBox[{"Count", "[", 
